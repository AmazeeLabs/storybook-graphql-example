<div class="page {{ node.gettype }} ">

  {% block header %}
    {% include '@storybook/navigation-search/search.html.twig' %}
  {% endblock %}

  {% block nav %}
    {% include '@storybook/menu/menu.html.twig' with nav %}
  {% endblock %}

  <div class="page__content-wrapper">
    <main class="page__main">
      {% block content %}
        {{ page.content }}
      {% endblock %}
    </main>
  </div>

</div>
